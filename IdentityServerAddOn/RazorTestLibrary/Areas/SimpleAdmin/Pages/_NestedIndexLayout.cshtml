@{
    Layout = "_Layout";
}
<div class="container">
    <table class="table table-striped border border-dark">
        <thead class="thead-dark">
            @RenderSection("TableHead", required: false)
        </thead>
        <tbody>
            @RenderSection("TableBody", required: false)
        </tbody>
        <caption>
            <div class="row">
                <div class="col-6">
                    <nav>
                        <ul class="pagination">
                            <li class="page-item active">
                                <a href="#" class="page-link addsign" data-toggle="modal" data-target="#addModal"></a>
                            </li>
                        </ul>
                    </nav>
                </div>
                <div class="col-6">
                    @RenderSection("Pagenation", required: false)
                </div>
            </div>
        </caption>
    </table>
</div>
