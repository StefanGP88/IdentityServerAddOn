@model List<string>

<thead class="thead-dark">
    <tr>
        <th class="align-middle">
            &nbsp;
        </th>
        @for (var i = 0; i < Model.Count; i++)
        {
            <th class="align-middle">
                <input type="text" class="form-control border-0 bg-transparent font-weight-bold text-white" value="@Model[i]" disabled/>
            </th>
        }
    <th class="align-middle">
        &nbsp;
    </th>
    </tr>
</thead>
