@model ResourcePropertyInfo
<div class="modal fade" id="@(Model.Name.ToLower())Modal" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="@(Model.Name.ToLower())ModalLabel">Add</h5>
                <button type="button" class="close" data-dismiss="modal">
                    <span>&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <partial name="@("FormPartials/_" + Model.Name)" />
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal"></button>
                <input type="submit" class="btn btn-info" value="&#10004;" form="#" onclick="@("Set"+Model.Name+"()")"/>
            </div>
        </div>
    </div>
</div>