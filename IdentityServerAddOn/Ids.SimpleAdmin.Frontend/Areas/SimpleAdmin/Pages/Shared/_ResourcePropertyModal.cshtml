@model ResourcePropertyInfo
<div class="modal fade" id="@(Model.Name)Modal" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="@(Model.Name)ModalLabel">Add</h5>
                <button type="button" class="close" data-dismiss="modal">
                    <span>&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <partial name="@Model.FormPartialName" />
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal"></button>
                <input type="submit" class="btn btn-info" form="AddForm" value="&#10004;" />
            </div>
        </div>
    </div>
</div>