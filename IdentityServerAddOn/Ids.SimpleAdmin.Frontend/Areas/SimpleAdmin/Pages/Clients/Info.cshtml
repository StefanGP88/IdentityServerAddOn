@page "{id:int?}"
@model Ids.SimpleAdmin.Frontend.Areas.SimpleAdmin.Pages.Clients.InfoModel
@{
    Layout = "_SimpleAdminLayout";
}

<partial name="_BaseSettings" model="@Model" />
<partial name="FormSections/_ScopeSection" model="@Model?.Data?.Scopes" />
<partial name="FormSections/_SecretSection" model="@Model?.Data?.Secrets?.Select(x => x as SecretsContract).ToList()" />
<partial name="_UriSection" model="@Model" />
<partial name="FormSections/_PropertySection" model="@Model?.Data?.Properties" />
<partial name="FormSections/_IdentityProviderSection" model="@Model?.Data?.IdPRestrictions" />
<partial name="FormSections/_GrantSection" model="@Model?.Data?.GrantTypes" />
<partial name="FormSections/_CorsSection" model="@Model?.Data?.CorsOrigins" />
<partial name="_ClaimsSection" model="@Model" />
<partial name="_LifeTimeSection" model="@Model" />